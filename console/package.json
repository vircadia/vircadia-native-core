  "devDependencies": {
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "0.36.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/highfidelity/hifi.git"
  },
  "main": "src/main.js",
  "scripts": {
